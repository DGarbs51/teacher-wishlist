{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "vercel-php"
      }
    ],
    "routes": [
      {
        "src": "/dist/(.*)",
        "dest": "dist/$1"
      },
      {
        "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
        "dest": "public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }