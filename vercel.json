{
    "version": 2,
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.0"
      }
    },
    "routes": [
      {
        "src": "/build/(.*)",
        "dest": "public/build/$1"
      },
      {
        "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
        "dest": "public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }